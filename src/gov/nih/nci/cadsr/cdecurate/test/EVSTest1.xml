        <entry key="name.2">blood</entry>

        <entry key="vocab.3">MedDRA</entry>
        <entry key="code.3">10005284</entry>
        <entry key="name.3">Blood ACTH [10005284]</entry>

        <entry key="vocab.4">VA_NDFRT</entry>
        <entry key="code.4">C11820</entry>
