<properties>
    
        <!-- Specify the caDSR database being used by the Curation Tool to be tested.
        -->
        <entry key="DSurl">cbiodb2-d.nci.nih.gov:1521:CBDEV</entry>
        <entry key="DSusername">guest</entry>
        <entry key="DSpassword">guest</entry>

        <!-- (optional) Specify the EVS URL for the caCORE API. If not included here it will be retrieved
            from the tool options table in the caDSR identified above.
        -->
        <entry key="evs.url">http://cabio-qa.nci.nih.gov/cacore31/http/remoteService</entry>

        <!-- Specify the sets of test data for the Vocabularies of interest. Each set is identified in the form
